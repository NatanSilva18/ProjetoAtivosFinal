
@{
    ViewData["Title"] = "Index";
}

@section scripts{
    <script type="text/javascript" src="js/Transferencia/Index.js?@DateTime.Now"></script>
    <script src="https://unpkg.com/jspdf-autotable"></script>

    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.2/js/dataTables.buttons.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.bootstrap4.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.html5.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jszip/3.1.3/jszip.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/pdfmake.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/pdfmake/0.1.53/vfs_fonts.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.print.min.js"></script>
    <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.6.2/js/buttons.colVis.min.js"></script>
}

@*Pesquisar Transferencias*@
<h2>Cadastro de Transferencias</h2>
<hr />
<div class="card mb-4 shadow p-3 mb-5 bg-white rounded">
    <div class="card-header">
        <i class="fa fa-table"></i>
        <span><b>Transferencia</b></span>
        <a class="float-right" href="javascript:$('#ajuda').modal('show');" data-toggle="tooltip" data-placement="top" title="Ajuda Transferencia de Ativos"><i class="fa fa-info"></i></a>
    </div>
    <div class="card-body">
        <form action="javascript:ObterTransferencias()">
            <input type="hidden" id="regional" value="@Context.Request.Cookies["Regiao"].ToString()" />
            <input type="hidden" id="filial" value="@Context.Request.Cookies["Filial"].ToString()" />
            <input type="hidden" id="regiaoFilial" value="@Context.Request.Cookies["RegiaoFilial"].ToString()" />
            <div class="form-group">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-xl-4">
                        <div class="form-group">
                            <div class="input-group">
                                <div class="input-group-append">
                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-info"></i></div>
                                </div>
                                <select id="cbbFilialOrigemPesquisa" name="cbbFilialOrigemPesquisa" class="form-control selectpicker" data-live-search="true" title="Não Selecionado" onchange="javascript: PrimeiraEtapa();">
                                    <option value="0" selected>Filial Origem</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-xl-4">
                        <div class="form-group">
                            <div class="input-group">
                                <div class="input-group-append">
                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-info"></i></div>
                                </div>
                                <select id="cbbFilialDestinoPesquisa" name="cbbFilialDestinoPesquisa" class="form-control selectpicker" data-live-search="true" title="Não Selecionado" onchange="javascript: PrimeiraEtapa();">
                                    <option value="0" selected>Filial Destino</option>
                                </select>
                            </div>
                        </div>

                    </div>
                    <div class="col-lg-4 col-md-4 col-xl-4 d-inline">
                        <button type="submit" id="btnPesquisar" name="btnPesquisar" class="btn btn-primary" title="Pesquisar Transferencias"><i class="fas fa-search"></i> <span>Pesquisar</span></button>
                        <button type="button" class="btn btn-success" onclick="javascript: $('#qtdAtivos').val('0'); LimparTransferencia();" data-toggle="modal" data-target="#novaTransferencia" title="Nova Transferencia"><i class="fas fa-plus"></i> <span>Transferencia</span></button>
                        <button type="button" class="btn btn-danger" onclick="javascript:LimparTabela()" title="Limpar Tabela Regionais"><i class="fas fa-ban"></i> <span>Cancelar</span></button>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-5">
                        <label><b><i class="fas fa-flag"></i> Status:</b></label>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdAtivo" id="rdAtivo1" value="1" checked>
                            <label class="form-check-label" for="rdAtivo1">Aberto</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdAtivo" id="rdAtivo2" value="0">
                            <label class="form-check-label" for="rdAtivo2">Fechado</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdAtivo" id="rdAtivo3" value="2">
                            <label class="form-check-label" for="rdAtivo3">Recusada</label>
                        </div>
                    </div>

                </div>
            </div>
        </form>
    </div>
</div>
<iframe id="txtArea1" style="display:none"></iframe>
@*Tabela Transferencia*@
<div id="tbbTransferencia" style="display:none" class="card mb-4">
    <div class="card-header">
        <i class="fa fa-table"></i>
        <span><b>Transferencias Cadastradas</b></span>
    </div>
    <div class="card-body">
        <div class="table-responsive-xl table-responsive-sm table-responsive-md">
            <table id="tableTransferencia" class="table table-sm table-hover" style="width:100%"></table>
        </div>
    </div>
</div>

@*Nova Transferencia*@
<input type="hidden" id="codigo" name="codigo" value="0" />
<div id="novaTransferencia" class="modal fade bd-example-modal-xl" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="novoTransferenciaLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Cadastrar Transferencias</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    <nav>
                        <div class="nav nav-tabs" id="nav-tab" role="tablist">
                            <a class="nav-item nav-link active" id="origemDestino" data-toggle="tab" href="#nav-OrigemDestino" role="tab" aria-controls="nav-OrigemDestino" aria-selected="true">1. Origem/Destino</a>
                            <a class="nav-item nav-link disabled" id="ativos" data-toggle="tab" href="#nav-ativos" role="tab" aria-controls="nav-ativos" aria-selected="false" onclick="javascript: SegundaEtapa();">2. Ativos</a>
                            <a class="nav-item nav-link disabled" id="documentos" data-toggle="tab" href="#nav-documentos" role="tab" aria-controls="nav-documentos" aria-selected="false" onclick="document.getElementById('confirmar').classList.remove('disabled');">3. Documentos</a>
                            <a class="nav-item nav-link disabled" id="confirmar" data-toggle="tab" href="#nav-confirmar" role="tab" aria-controls="nav-confirmar" aria-selected="false" onclick="javascript: ConfirmarCampos();">4. Resumo</a>
                        </div>
                    </nav>
                    <div class="tab-content" id="nav-tabContent">
                        <div class="tab-pane fade show active" id="nav-OrigemDestino" role="tabpanel" aria-labelledby="nav-home-tab">
                            <div class="form-group">
                                <hr />
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label for="cbbMotivo">Motivo *</label>
                                            <div class="input-group">
                                                <div class="input-group-append">
                                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-info"></i></div>
                                                </div>
                                                <select id="cbbMotivo" name="cbbMotivo" class="form-control selectpicker" data-live-search="true" title="Não Selecionado" onchange="javascript: PrimeiraEtapa();">
                                                    <option value="" selected></option>
                                                </select>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="col-lg-8">
                                        <div class="form-group">
                                            <label for="txtDescricao">Descricao Transferencia *</label>
                                            <input style="resize: none;" class="form-control" id="txtDescricao" required autocomplete="off" maxlength="50" onchange="javascript: PrimeiraEtapa();">
                                        </div>
                                    </div>
                                </div>
                                <h6>Origem</h6>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label for="cbbFilialOrigem">Filial Origem *</label>
                                            <div class="input-group">
                                                <div class="input-group-append">
                                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                </div>
                                                <select id="cbbFilialOrigem" name="cbbFilialOrigem" class="form-control selectpicker" data-live-search="true" title="Não Selecionado" onchange="javascript: LimparTabelaItens(); LimparCombo('cbbAtivos'); BuscarAtivos(this); ObterResponsavel(this.value, 1); PrimeiraEtapa();">
                                                    <option value="" selected></option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label for="txtResponsavelOrigem">Responsavel *</label>
                                            <div class="input-group">
                                                <div class="input-group-append">
                                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-user"></i></div>
                                                </div>
                                                <input class="form-control" type="text" name="txtResponsavelOrigem" id="txtResponsavelOrigem" disabled />
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <h6>Destino</h6>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label for="cbbFilialDestino">Filial Destino *</label>
                                            <div class="input-group">
                                                <div class="input-group-append">
                                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                </div>
                                                <select id="cbbFilialDestino" name="cbbFilialDestino" class="form-control selectpicker" data-live-search="true" title="Não Selecionado" onchange="javascript: LimparCombo('cbbLocalDestino'); BuscarLocaisDestino(this); ObterResponsavel(this.value, 2); PrimeiraEtapa();">
                                                    <option value="" selected></option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label for="txtResponsavelDestino">Responsavel Destino *</label>
                                            <div class="input-group">
                                                <div class="input-group-append">
                                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-user"></i></div>
                                                </div>
                                                <input class="form-control" type="text" name="txtResponsavelDestino" id="txtResponsavelDestino" disabled />
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="float-right">
                                <span class="d-inline-block" tabindex="0" data-toggle="tooltip" title="Preencha Todos os Campos">
                                    <button class="btn btn-info" disabled id="btProx1" type="button" style="margin: 0 auto; pointer-events: none;" onclick="javascript: BotoesProximo('origemDestino', 'ativos', 'nav-OrigemDestino', 'nav-ativos');">  <i class="fa fa-arrow-right"></i> Proximo</button>
                                </span>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="nav-ativos" role="tabpanel" aria-labelledby="nav-ativos-tab">
                            <hr />
                            <div class="form-group">

                                <div id="mostrandoDE">
                                    @*script*@
                                </div>
                                <form id="SelecionarItens" action="javascript: AdicionarAtivos();">
                                    <div class="row">
                                        <input type="hidden" id="qtdAtivos" value="0" />
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="cbbAtivos">Ativos *</label>
                                                <div class="input-group">
                                                    <div class="input-group-append">
                                                        <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                    </div>
                                                    <select id="cbbAtivos" name="cbbAtivos" class="form-control selectpicker" required data-live-search="true" title="Selecione" onchange="javascript: SegundaEtapa();">
                                                        <option value="" selected></option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-6">
                                            <div class="form-group">
                                                <label for="fuArquivo">Foto Estado *</label>

                                                <div class="input-group" style="margin-top:0%">
                                                    <div class="custom-file" id="customFile" lang="pt-br">
                                                        <input type="file" class="custom-file-input" id="fuArquivo" aria-describedby="fileHelp" required onchange="javascript: SegundaEtapa();">
                                                        <label class="custom-file-label" for="fuArquivo" id="labelFoto">
                                                            Selecione a Foto
                                                        </label>
                                                    </div>
                                                    <div class="input-group-append">
                                                        <button class="btn btn-success" type="submit" onclick="javascript: SalvarFotos();" id="btnSalvarFotos">Adicionar</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="divAlerta" class="alert alert-danger col-lg-6" role="alert" style="display:none"></div>
                                    </div>
                                </form>

                                @*Tabela Itens Transferencia*@
                                <div id="tbbItensTransferencia" style="display:none" class="card mb-4">
                                    <div class="card-header">
                                        <i class="fa fa-table"></i>
                                        <span><b>Ativos Transferencia</b></span>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive-xl table-responsive-sm table-responsive-md">
                                            <table id="tableItensTransferencia" class="table table-sm table-hover" style="width:100%"></table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="float-left">
                                <button class="btn btn-info" id="btVoltar1" type="button" onclick="javascript: BotoesProximo('ativos', 'origemDestino', 'nav-ativos', 'nav-OrigemDestino');" style="margin: 0 auto" data-toggle="tooltip" data-placement="top" title="Voltar Origem/Destino"><i class="fa fa-arrow-left"></i> Voltar</button>
                            </div>
                            <div class="float-right">
                                <span class="d-inline-block" tabindex="0" data-toggle="tooltip" title="Preencha Todos os Campos">
                                    <button class="btn btn-info" id="btProx2" disabled type="button" style="margin: 0 auto; pointer-events: none;" onclick="javascript: BotoesProximo('ativos', 'documentos', 'nav-ativos', 'nav-documentos'); document.getElementById('confirmar').classList.remove('disabled');">  <i class="fa fa-arrow-right"></i> Proximo</button>
                                </span>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="nav-documentos" role="tabpanel" aria-labelledby="nav-documentos-tab">

                            <hr />
                            <div class="form-group">
                                <form id="FormDocs" action="">
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="form-group">
                                                <label for="fuArquivoDoc">Documentos</label>

                                                <div class="input-group" style="margin-top:0%">
                                                    <div class="custom-file" id="customFile" lang="pt-br">
                                                        <input type="file" class="custom-file-input" id="fuArquivoDoc" aria-describedby="fileHelp" multiple>
                                                        <label class="custom-file-label" for="fuArquivoDoc" id="labelFotoRec">
                                                            Selecione o Anexo
                                                        </label>
                                                    </div>
                                                    <div class="input-group-append">
                                                        <button class="btn btn-success" type="button" onclick="javascript: SalvarDocs();" id="btnSalvarDocumentos">Adicionar</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div id="divAlertaDoc" class="alert alert-danger col-lg-6" role="alert" style="display:none"></div>
                                    </div>
                                    <input type="hidden" id="meuAnexoHidden" name="meuAnexoHidden" value="" />
                                    <input type="hidden" id="txtQtd" name="txtQtd" value="0" />

                                    @*Tabela docs. Transferencia*@
                                    <div class="card mb-4 shadow p-3 mb-5 bg-white rounded" style="display:none" id="modalDocs">
                                        <div class="card-header">
                                            <i class="fa fa-table"></i>
                                            <span><b>Anexos Adiconados</b></span>
                                        </div>
                                        <div class="card-body" id="cardDocs" style="display:none">
                                            <div class="row" id="Doc">
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                            <hr />
                            <div class="float-left">
                                <button class="btn btn-info" type="button" id="btVoltar2" onclick="javascript: BotoesProximo('documentos', 'ativos', 'nav-documentos', 'nav-ativos');" style="margin: 0 auto" data-toggle="tooltip" data-placement="top" title="Voltar p/ Ativos"><i class="fa fa-arrow-left"></i> Voltar</button>
                            </div>
                            <div class="float-right">
                                <button class="btn btn-info" type="button" id="btnProx3" onclick="javascript: BotoesProximo('documentos', 'confirmar', 'nav-documentos', 'nav-confirmar'); ConfirmarCampos();" style="margin: 0 auto" data-toggle="tooltip" data-placement="top" title="Ir p/ Confirmar"><i class="fa fa-arrow-right"></i> Proximo</button>
                            </div>
                        </div>
                        <div class="tab-pane fade" id="nav-confirmar" role="tabpanel" aria-labelledby="nav-confirmar-tab">
                            <div class="form-group" id="confirmarCampos">

                            </div>
                            <div id="tbbConfirmarItens" style="display:none" class="card mb-4">
                                <div class="card-header">
                                    <i class="fa fa-table"></i>
                                    <span><b>Ativos Transferencia</b></span>
                                </div>
                                <div class="card-body">
                                    <div class="table-responsive-xl table-responsive-sm table-responsive-md">
                                        <table id="tableConfirmarItens" class="table table-sm table-hover" style="width:100%"></table>
                                    </div>
                                </div>
                            </div>
                            <div id="tbbConfirmarDocs" style="display:none" class="card mb-4">
                                <div class="card-header">
                                    <i class="fa fa-table"></i>
                                    <span><b>Documentos Transferencia</b></span>
                                </div>
                                <div class="card-body">
                                    <div class="row" id="confirmarDocs">

                                    </div>
                                </div>
                            </div>
                            <hr />
                            <div class="float-left">
                                <button class="btn btn-info" type="button" id="btVoltar3" onclick="javascript: BotoesProximo('confirmar', 'documentos', 'nav-confirmar', 'nav-documentos');" style="margin: 0 auto" data-toggle="tooltip" data-placement="top" title="Voltar p/ Documentos"><i class="fa fa-arrow-left"></i> Voltar</button>
                            </div>
                            <div class="float-right">
                                <button class="btn btn-success" type="button" style="margin: 0 auto" onclick="Gravar()">  <i class="fa fa-check"></i> Gravar Transferência</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>
@*ajuda Transferencia*@
