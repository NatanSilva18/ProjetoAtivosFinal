@*Pesquisar Ativos*@
<div class="d-sm-flex align-items-center justify-content-between mb-2">
    <h1 class="h3 mb-0 text-gray-800">Cadastro de Ativos</h1>
</div>
<hr />

<div class="card mb-4 shadow p-3 mb-5 bg-white rounded">
    <div class="card-body">
        <form action="javascript:ObterAtivos()">
            <input type="hidden" id="regional" value="@Context.Request.Cookies["Regiao"].ToString()" />
            <input type="hidden" id="filial" value="@Context.Request.Cookies["Filial"].ToString()" />
            <input type="hidden" id="regiaoFilial" value="@Context.Request.Cookies["RegiaoFilial"].ToString()" />
            <div class="form-group">
                <div class="row">
                    <div class="col-lg-3 col-md-3 col-xl-3">
                        <div class="form-group">
                            <div class="input-group">
                                <div class="input-group-append">
                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                </div>
                                <input type="search" name="txtPesquisar" id="txtPesquisar" class="form-control" placeholder="Pesquisar" value="" autofocus autocomplete="off" />

                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3 col-md-3 col-xl-3">
                        <div class="form-group">
                            <div class="input-group">
                                <div class="input-group-append">
                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                </div>
                                <select id="cbbRegiaoPesq" name="cbbRegiaoPesq " data-live-search="true" class="form-control selectpicker" required onchange="CarregarFiliaisPesq(this.value);">
                                    <option value="" selected>Região</option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-2 col-md-2 col-xl-2">
                        <div class="form-group">
                            <div class="input-group">
                                <div class="input-group-append">
                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                </div>
                                <select id="cbbFilialPesq" name="cbbFilialPesq" class="form-control selectpicker" data-live-search="true">
                                    <option value="0" selected>Filiais Pertecente a Região Selecionado</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-4 col-md-4 col-xl-4 d-inline">
                        <button type="submit" id="btnPesquisar" name="btnPesquisar" class="btn btn-primary" data-toggle="tooltip" data-placement="top" title="Pesquisar Regionais"><i class="fas fa-search"></i> <span>Pesquisar</span></button>
                        <button type="button" class="btn btn-success" data-toggle="modal" data-target="#novoAtivo" onclick="javascript: LimparCampos(); LockFields();" title="Novo Ativo"><i class="fas fa-plus"></i> <span>Ativo</span></button>
                        <button type="button" class="btn btn-danger" onclick="javascript:LimparTabela()" title="Limpar Tabela Regionais"><i class="fas fa-ban"></i> <span>Cancelar</span></button>
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-3 col-md-3 col-xl-3">
                        <label><b><i class="fas fa-filter"></i> Filtro:</b></label>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdFiltro" id="rdFiltro1" value="Nome" checked onchange="AlterarRequeridoNome();">
                            <label class="form-check-label" for="rdFiltro1">Nome</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdFiltro" id="rdFiltro2" value="Placa" onchange="AlterarRequeridoPlaca();">
                            <label class="form-check-label" for="rdFiltro2">Placa</label>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-7 col-xl-7">
                        <label><b><i class="fas fa-flag"></i> Status:</b></label>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdAtivo" id="rdAtivo1" value="1" checked>
                            <label class="form-check-label" for="rdAtivo1">Ativo</label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="rdAtivo" id="rdAtivo2" value="0">
                            <label class="form-check-label" for="rdAtivo2">Inativo</label>
                        </div>
                    </div>

                </div>

            </div>
        </form>
    </div>
</div>
<iframe id="txtArea1" style="display:none"></iframe>
@*Tabela Ativo*@
<div id="tbbAtivo" style="display:none" class="card mb-4">
    <div class="card-header">
        <i class="fa fa-table"></i>
        <span><b>Ativos Cadastrados</b></span>
    </div>
    <div class="card-body">
        <div class="table-responsive-xl table-responsive-sm table-responsive-md">
            <table id="tableAtivo" class="table table-sm table-hover" style="width:100%"></table>
        </div>
    </div>
</div>


@*Novo Ativo*@
<div id="novoAtivo" class="modal fade bd-example-modal-xl" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="novoAtivoLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="staticBackdropLabel">Cadastrar Ativos</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="container-fluid">
                    
                    <form name="formRegional" id="formRegional" action="javascript: Gravar();" method="post" >
                        <input type="hidden" id="minhaImagemHidden" name="minhaImagemHidden" value="" />
                        <input type="hidden" id="validaPlaca" name="validaPlaca" value="0" />

                        <div class="form-group">
                            <label for="cbbTpAtivo">Tipo Ativo <label style="color:red">*</label></label>
                            <div class="input-group">
                                <div class="input-group-append">
                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                </div>
                                <select id="cbbTpAtivo" name="cbbTpAtivo" class="form-control selectpicker" data-live-search="true" required onchange="PreencherValor(this);">
                                    <option value="" selected>Tipo Ativo</option>
                                </select>
                            </div>
                        </div>
                        <hr />
                        <div class="form-group">
                            <input type="hidden" name="txtId" id="txtId" value="0" />

                            <div class="card mb-4 shadow p-3 mb-5 bg-white rounded" id="cardRegional">
                                <div class="card-header">
                                    <i class="fa fa-table"></i>
                                    <span><b>Regionais, Filiais e Salas</b></span>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-lg-4">
                                            <div class="form-group">
                                                <label for="cbbRegional">Regional <label style="color:red">*</label></label>
                                                <div class="input-group">
                                                    <div class="input-group-append">
                                                        <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                    </div>
                                                    <select id="cbbRegional" name="cbbRegional" class="form-control selectpicker" data-live-search="true" required onchange="javascript:LimparCombo('cbbFilial');LimparCombo('cbbSala'); CarregarFiliais(this); ">
                                                        <option value="" selected>Regionais</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4" id="linhaFilial" style="display:none">
                                            <div class="form-group">
                                                <label for="cbbFilial">Filial <label style="color:red">*</label></label>
                                                <div class="input-group">
                                                    <div class="input-group-append">
                                                        <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                    </div>
                                                    <select id="cbbFilial" name="cbbFilial" class="form-control selectpicker" data-live-search="true" onchange="javascript:LimparCombo('cbbSala'); BuscarSalas(this);">
                                                        <option value="" selected>Filiais da Regional</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-lg-4" id="sala" style="display:none">
                                            <div class="form-group">
                                                <label for="cbbSala">Local <label style="color:red">*</label></label>
                                                <div class="input-group">
                                                    <div class="input-group-append">
                                                        <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                    </div>
                                                    <select id="cbbSala" name="cbbSala" class="form-control selectpicker" data-live-search="true">
                                                        <option value="" selected>Salas da Filial</option>
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div id="divAlertaPlaca" class="alert alert-danger col-lg-8" role="alert" style="display:none"></div>

                            </div>
                            <input type="hidden" name="txtIdNotaFiscal" id="txtIdNotaFiscal" value="0" />

                            <div class="card card-body" style="border-radius: 4px; border-left: 4px solid blue;">
                                <div class="d-sm-flex align-items-center justify-content-between mb-0">
                                    <h1 class="h6 mb-0 text-gray-800">Dados Fiscais</h1>
                                </div>
                                <hr />
                                <div class="row">
                                    <div class="col-lg-4">
                                        <div class="form-group">
                                            <label for="txtNumeroNota">Nº Nota <label style="color:red">*</label></label>
                                            <div class="input-group">
                                                <div class="input-group-append">
                                                    <div class="input-group-text" style="background-color: #FFF"><i class="fas fa-building"></i></div>
                                                </div>
                                                <input class="form-control" type="number" name="txtNumeroNota" id="txtNumeroNota" autocomplete="off" maxlength="30" required />
                                            </div>

                                        </div>
                                    </div>
                                    <div class="col-lg-4">
